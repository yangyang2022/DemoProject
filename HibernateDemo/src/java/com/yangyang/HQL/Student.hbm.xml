<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yangyang.HQL">
    <class name="Student" table="t_stu">
        <cache usage="read-only" region="ehcache.xml" />
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="name" />
        <property name="sex" />
        <many-to-one name="classroom" column="c_id"/>
    </class>
</hibernate-mapping>
